<view style="padding-bottom:40rpx;"><uni-swiper-dot vue-id="1" info="{{listData}}" current="{{current}}" dots-styles="{{dotsStyles}}" mode="{{mode}}" field="title" bind:__l="__l" vue-slots="{{['default']}}"><swiper class="swiper-box" autoplay="true" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image class="image" src="{{item.coverUrl}}" mode="aspectFill"></image></swiper-item></block></swiper></uni-swiper-dot><view class="uni-list"><block wx:for="{{listData}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view class="uni-list-cell" hover-class="uni-list-cell-hover"><view class="uni-media-list"><image class="uni-media-list-logo" src="{{value.coverUrl}}"></image><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{value.title}}</view><view class="uni-media-list-text-bottom"><text style="float:left;">{{value.updateTime+" 更新"}}</text><text data-event-opts="{{[['tap',[['toDelete',['$0'],[[['listData','',key]]]]]]]}}" style="margin-right:40rpx;" bindtap="__e">删除</text><text data-event-opts="{{[['tap',[['toEdit',['$0'],[[['listData','',key]]]]]]]}}" bindtap="__e">编辑 »</text></view></view></view></view></block></view><view data-event-opts="{{[['tap',[['toAdd']]]]}}" class="uni-fab" bindtap="__e"><view class="uni-fab__content"><view class="uni-fab__item uni-fab__item--first"></view></view><view class="uni-fab__circle uni-fab__plus"><view class="fab-circle-v"></view><view class="fab-circle-h"></view></view></view></view>